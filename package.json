{
  "name": "vue-chess2",
  "version": "1.0.2",
  "description": "Multiplayer online chess game",
  "main": "index.js",
  "scripts": {
    "prod:build": "webpack -p",
    "prod": "node dist/main.js",
    "build": "webpack",
    "build:watch": "webpack --watch --watch-poll",
    "pretest": "npm run build",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "test:notify": "npm run test:watch -- --notify",
    "coverage": "npm test -- --coverage",
    "coverage:notify": "npm run coverage -- --watch --notify",
    "start": "npm run build && node dist/main.js",
    "start:watch": "concurrently \"npm run build:watch\" \"node-dev dist/main.js\"",
    "preupver": "npm test",
    "upver": "standard-version"
  },
  "keywords": [
    "vue",
    "webpack",
    "mongodb",
    "graphql",
    "express",
    "apollo-server",
    "typescript",
    "graphiql"
  ],
  "author": "gustaYo",
  "license": "ISC",
  "dependencies": {
    "@types/body-parser": "1.16.3",
    "@types/cors": "2.8.1",
    "@types/express": "^4.0.35",
    "@types/helmet": "0.0.36",
    "@types/morgan": "^1.7.32",
    "app-root-dir": "^1.0.2",
    "async": "^2.4.1",
    "body-parser": "^1.17.1",
    "cors": "^2.8.3",
    "ejs": "^2.5.6",
    "express": "^4.15.2",
    "express-ts-controller": "^0.1.2",
    "express-useragent": "^1.0.7",
    "graphql": "0.9.6",
    "apollo-server-express": "^1.0.5",
    "graphql-subscriptions": "^0.4.1",
    "graphql-tag": "^2.1.0",
    "graphql-tools": "0.11.0",
    "helmet": "^3.6.0",
    "install": "^0.10.1",
    "ip": "^1.1.5",
    "lodash": "^4.17.4",
    "merge-graphql-schemas": "0.0.15",
    "moment": "^2.18.1",
    "mongodb": "^2.2.27",
    "mongoose": "^4.10.2",
    "morgan": "^1.8.1",
    "npm": "^4.6.1",
    "optics-agent": "^1.1.3",
    "orm": "^3.2.3",
    "q": "^1.5.0",
    "request": "^2.81.0",
    "request-ip": "^2.0.1",
    "rotating-file-stream": "^1.2.2",
    "rxjs": "^5.4.0",
    "subscriptions-transport-ws": "^0.7.0"
  },
  "devDependencies": {
    "@types/graphql": "^0.10.2",
    "@types/jest": "^20.0.6",
    "@types/mongodb": "^2.2.2",
    "@types/mongoose": "^4.7.13",
    "@types/node": "^8.0.19",
    "awesome-typescript-loader": "^3.2.2",
    "concurrently": "^3.4.0",
    "jest": "^20.0.1",
    "jest-cli": "^20.0.1",
    "node-dev": "^3.1.3",
    "standard-version": "^4.0.0",
    "ts-jest": "^20.0.10",
    "tslint": "^5.6.0",
    "tslint-loader": "^3.5.3",
    "typescript": "2.3.2",
    "webpack": "^3.4.1",
    "webpack-node-externals": "^1.6.0"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "mapCoverage": true,
    "testEnvironment": "node",
    "testRegex": ".*\\.spec\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ]
  }
}
